<p id="notice"><%= notice %></p>

<h1>Listing Day Collections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @day_collections.each do |day_collection| %>
      <tr>
        <td><%= link_to 'Show', day_collection %></td>
        <td><%= link_to 'Edit', edit_day_collection_path(day_collection) %></td>
        <td><%= link_to 'Destroy', day_collection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Day collection', new_day_collection_path %>
